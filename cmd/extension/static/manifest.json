{
  "manifest_version" : 2,
  "name" : "Weblink Devtools",
  "version" : "0.1",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "description" : "Frontend Tools for Weblink",
  "author" : "David Alsh",
  "devtools_page" : "devtools/index.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "match_about_blank" : true,
      "js": ["content/index.js"],
      "all_frames": true
    }
  ]
}